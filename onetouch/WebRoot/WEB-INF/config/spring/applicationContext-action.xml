	<bean name="/downloadinfo" class="com.tcl.onetouch.action.DownloadinfoAction"/>
	<bean name="/logs" class="com.tcl.onetouch.action.LogsAction"/>
	<bean name="/types" class="com.tcl.onetouch.action.TypesAction"/>
	<bean name="/systempara" class="com.tcl.onetouch.action.SystemparameterAction"/>
	<bean name="/ad" class="com.tcl.onetouch.action.AdAction"/>
	<bean name="/gameLanguage" class="com.tcl.onetouch.action.GameLanguageAction"/>
	<!-- wap -->
	<bean name="/index" class="com.tcl.onetouch.wap.LoginAction"/>
	<!-- 中国移动游戏基地 -->
	<bean name="/chinaMobileGame" class="com.tcl.onetouch.chinaMobile.GameAction"/>
