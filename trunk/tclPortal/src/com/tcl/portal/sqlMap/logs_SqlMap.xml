<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="logs" >
  <resultMap id="logsResult" class="com.tcl.portal.domain.Logs" >
    <result column="id" property="id" jdbcType="INTEGER" />
    <result column="userId" property="userid" jdbcType="INTEGER" />
    <result column="doSomthing" property="dosomthing" jdbcType="VARCHAR" />
    <result column="ltime" property="ltime" jdbcType="TIMESTAMP" />
  </resultMap>
   <select id="findLogsCount" parameterClass="java.util.Map" resultClass="java.lang.Integer" >
    		select count(*) from logs
  </select>
  
  <select id="findLogsList" parameterClass="java.util.Map" resultMap="logsResult">
	  select * from logs order by id desc limit #start#,#end#
  </select>
</sqlMap>