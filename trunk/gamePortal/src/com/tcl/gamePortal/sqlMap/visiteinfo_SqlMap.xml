<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="visiteinfo" >
  <resultMap id="visiteinfoResult" class="com.tcl.gamePortal.domain.Visiteinfo" >
    <result column="id" property="id" jdbcType="INTEGER" />
    <result column="ip" property="ip" jdbcType="VARCHAR" />
    <result column="telephone" property="telephone" jdbcType="VARCHAR" />
    <result column="did" jdbcType="VARCHAR" property="did" />
    <result column="titleId" property="titleid" jdbcType="INTEGER" />
    <result column="gameId" property="gameid" jdbcType="INTEGER" />
    <result column="location" property="location" jdbcType="INTEGER" />
    <result column="vtime" property="vtime" jdbcType="TIMESTAMP" />
  </resultMap>
  <insert id="saveVisiteinfo" parameterClass="com.tcl.gamePortal.domain.Visiteinfo">
    insert into visiteinfo (id, ip, telephone, did, titleId, gameId, location, vtime)
    values (#id:INTEGER#, #ip:VARCHAR#, #telephone:VARCHAR#, #did:VARCHAR#, #titleid:INTEGER#,
      #gameid:INTEGER#, #location:INTEGER#, #vtime:TIMESTAMP#)
  </insert>
</sqlMap>