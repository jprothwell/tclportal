<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="types" >
  <resultMap id="typesResult" class="com.tcl.gamePortal.domain.Types" >
    <result column="id" property="id" jdbcType="INTEGER" />
    <result column="fid" property="fid" jdbcType="INTEGER" />
    <result column="typeValue" property="typevalue" jdbcType="VARCHAR" />
    <result column="allowance" property="allowance" jdbcType="VARCHAR" />
  </resultMap>
 
  
   <select id="queryType" parameterClass="java.lang.Integer" resultClass="java.lang.String">
	  select typeValue from types where id=#kindid#
  </select>
  
  


</sqlMap>