<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="gameresource" >
  <resultMap id="gameresourceResult" class="com.tcl.paypal.domain.Gameresource" >
    <result column="id" jdbcType="INTEGER" property="id" />
    <result column="name" jdbcType="VARCHAR" property="name" />
    <result column="images" jdbcType="VARCHAR" property="images" />
    <result column="file1" jdbcType="VARCHAR" property="file1" />
    <result column="file2" jdbcType="VARCHAR" property="file2" />
    <result column="ua" jdbcType="VARCHAR" property="ua" />
    <result column="spid" jdbcType="INTEGER" property="spid" />
    <result column="sdate" jdbcType="TIMESTAMP" property="sdate" />
    <result column="lg" jdbcType="VARCHAR" property="lg" />
    <result column="kindid" jdbcType="INTEGER" property="kindid" />
    <result column="typeid" jdbcType="INTEGER" property="typeid" />
    <result column="rel" jdbcType="TINYINT" property="rel" />
    <result column="orderid" jdbcType="INTEGER" property="orderid" />
    <result column="sizeid" jdbcType="SMALLINT" property="sizeid" />
    <result column="price" jdbcType="VARCHAR" property="price" />
  </resultMap>
 
  <select id="queryGameresource" resultMap="gameresourceResult" parameterClass="java.lang.Integer" >
    select * from gameresource where id = #id:INTEGER#
  </select>
</sqlMap>