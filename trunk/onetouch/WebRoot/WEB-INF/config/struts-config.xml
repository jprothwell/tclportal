	       <form-bean name="spinfoForm" type="com.tcl.onetouch.form.SpinfoForm"></form-bean>
	       <form-bean name="ipinfoForm" type="com.tcl.onetouch.form.IpinfoForm"></form-bean>
	       <form-bean name="javaparameterForm" type="com.tcl.onetouch.form.JavaparameterForm"></form-bean>
